{
    "name": "rozetka-tests",
    "author": "chickenson",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "allure-commandline": "^2.18.1",
        "mocha": "^10.0.0",
        "wdio": "^6.0.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.30.3",
        "@typescript-eslint/parser": "^5.30.3",
        "@wdio/allure-reporter": "^7.20.7",
        "@wdio/cli": "^7.20.5",
        "@wdio/local-runner": "^7.20.5",
        "@wdio/mocha-framework": "^7.20.3",
        "@wdio/spec-reporter": "^7.20.3",
        "chromedriver": "^103.0.0",
        "eslint": "^8.19.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-mocha": "^10.0.5",
        "eslint-plugin-n": "^15.2.3",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-wdio": "^7.19.4",
        "husky": "^8.0.1",
        "mocha-eslint": "^7.0.0",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4",
        "wdio-allure-ts": "^6.4.0",
        "wdio-chromedriver-service": "^7.3.2",
        "wdio-wait-for": "^2.2.6"
    },
    "scripts": {
        "test": "wdio run wdio.conf.ts && npm run report",
        "searchTest": "wdio run wdio.conf.ts --spec search.spec.ts",
        "cartAddTest": "wdio wdio.conf.ts --suite cartAdd",
        "cartDeleteTest": "wdio run wdio.conf.ts --spec cart.delete.spec.ts",
        "cartRefreshTest": "wdio run wdio.conf.ts --spec cart.refresh.spec.ts",
        "cartAmountTest": "wdio run wdio.conf.ts --spec cart.amount.spec.ts",
        "entryTest": "wdio run wdio.conf.ts --spec entry.spec.ts",
        "lint": "eslint . --ext .ts .",
        "lint:fix": "eslint . --ext .ts . --fix",
        "compile": "tsc --outDir tmp",
        "prepare": "husky install",
        "report": "allure generate allure-results/ --clean && allure open"
    }
}
